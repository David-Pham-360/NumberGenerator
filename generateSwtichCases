def generate_switch_case(num_cases):
    code = ""
    for i in range(num_cases, num_cases + 100):
        code += f"    {'elif' if i > 1 else 'if'} number == {i}:\n"
        code += f"        print({'Odd' if i % 2 != 0 else 'Even'})\n"
    return code

num_cases = 10000000  # Change this value to generate code for a different number of cases
generated_code = generate_switch_case(num_cases)
print(generated_code)

